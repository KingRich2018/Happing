<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.happing.domain.mapper.HistroyMapper">

    <select id="getToday" parameterType="java.lang.String" resultType="Histroy">
        select * FROM tb_histroy where date=#{date} ORDER BY time DESC
    </select>




</mapper>